{
  "manifest_version": 3,
  "name": "ZSK Scrapper",
  "description": "Teacher Timetable Scrapper",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "majc.png"
  },
  "background":{
    "service_worker": "background.js",
    "type": "module" 
  },
  "permissions":[
    "storage"
  ],
  "host_permissions":[
    "https://*/*",
    "http://*/*"
  ]
}